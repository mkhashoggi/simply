{{!-- Email subscribe form at the Top of the page --}}
<div class="subscribeBanner">
    <div class="container">
        <div class="row">
            <div class="col s12 m8 u-flexCenter u-md-block">
                <div class="subscribeBanner-title u-fontWeightMedium u-relative u-fontSize28 u-paddingRight20">{{t "Contribute with us"}}</div>
                <div>{{t "Stay up to date! Get all the latest & greatest posts delivered straight to your inbox"}}</div>
            </div>
            <div class="col s12 m4 u-textAlignLeft">
                <div class="buttonSet">
                    <div class="button button--outline-success u-md-block js-m-subscribe-toggle">
                        {{t "Subscribe To Newsletter"}}
                    </div>
                    {{#link href="/about/" class="u-textCapitalize button button--primary"}}{{t "Volunteer with us"}}{{/link}}
                </div>
            </div>
        </div>
    </div>
</div>

